;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Project07"
config/features=PackedStringArray("4.4", "Forward Plus")

[autoload]

Globals="*res://globals/globals.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/pattern_2d_editor/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "orange",
"res://globals/": "gray",
"res://resources/": "green",
"res://scenes/": "yellow",
"res://shaders/": "pink"
}

[rendering]

textures/canvas_textures/default_texture_filter=0
